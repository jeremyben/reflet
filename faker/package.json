{
	"name": "@reflet/faker",
	"version": "1.0.0",
	"author": "Jeremy Bensimon",
	"license": "MIT",
	"repository": "https://github.com/jeremyben/reflet/tree/master/faker",
	"description": "Well-defined and well-typed faker decorators",
	"keywords": [
		"faker",
		"decorators",
		"typescript"
	],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=8.10"
	},
	"engineStrict": true,
	"peerDependencies": {
		"@types/faker": "^4",
		"@types/node": ">=8",
		"faker": "^4",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@types/faker": "^4.1.11",
		"faker": "^4.1.0"
	},
	"scripts": {
		"build": "ts-node -T ../build.ts",
		"test": "jest --config ../jest.config.js",
		"test:watch": "yarn test --watch --verbose false",
		"test:file": "yarn test --testPathPattern",
		"preversion": "yarn test --coverage && ts-node -T ../testing/summary.ts && git add coverage-summary.json",
		"prepublishOnly": "yarn run build"
	}
}
