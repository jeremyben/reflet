{
	"name": "@reflet/eslint-decorators",
	"version": "1.0.0",
	"author": "Jeremy Bensimon",
	"license": "MIT",
	"repository": "https://github.com/jeremyben/reflet/tree/master/eslint-decorators",
	"description": "Well-defined and well-typed eslint-decorators decorators",
	"keywords": [
		"eslint",
		"eslintplugin",
		"express",
		"decorators",
		"typescript"
	],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=10"
	},
	"engineStrict": true,
	"dependencies": {
		"path-to-regexp": "^6.2.0"
	},
	"peerDependencies": {
		"@reflet/express": "^1.6.0",
		"eslint": "^7.32.0"
	},
	"devDependencies": {
		"@reflet/express": "^1.6.0",
		"@types/node": "14",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/experimental-utils": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"eslint": "^7.32.0"
	},
	"scripts": {
		"build": "ts-node -T ../build.ts",
		"test": "jest --config ../jest.config.js",
		"test:watch": "yarn test --watch --verbose false",
		"test:file": "yarn test --testPathPattern",
		"preversion": "yarn test --coverage && ts-node -T ../testing/summary.ts && git add coverage-summary.json",
		"prepublishOnly": "yarn run build"
	}
}
